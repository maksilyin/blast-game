{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "typeRoots": [
      "./node_modules/phaser/types",
      "./src"
    ],
    "types": [
      "Phaser",
      "./src/types"
    ],
    "lib": [
      "ES2016",
      "dom"
    ]
  }
}
